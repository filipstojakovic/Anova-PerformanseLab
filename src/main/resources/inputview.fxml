<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="268.0" prefWidth="370.0" style="-fx-background-color: #2980b9;" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="main.InputViewController">
    <children>
        <Label layoutX="120.0" layoutY="38.0" text="ANOVA &amp; Kontrasti" textFill="WHITE">
         <font>
            <Font name="System Bold" size="14.0" />
         </font></Label>
        <Label layoutX="27.0" layoutY="102.0" text="Broj Mjerenja:" textFill="WHITE">
         <font>
            <Font name="System Bold" size="14.0" />
         </font>
      </Label>
        <TextField fx:id="numOfMeasurements" layoutX="153.0" layoutY="97.0" text="2">
         <font>
            <Font size="14.0" />
         </font></TextField>
        <Label layoutX="27.0" layoutY="145.0" text="Broj Alternativa:" textFill="WHITE">
         <font>
            <Font name="System Bold" size="14.0" />
         </font>
      </Label>
        <TextField fx:id="numOFAlternatives" layoutX="153.0" layoutY="140.0" text="2">
         <font>
            <Font size="14.0" />
         </font></TextField>
        <Button layoutX="145.0" layoutY="207.0" mnemonicParsing="false" onAction="#submitClicked" prefHeight="37.0" prefWidth="80.0" style="-fx-background-color: #2ecc71;" text="Potvrdi">
